---
import { cn } from "@/lib/utils";

interface Props {
  class?: string;
}

const { class: className } = Astro.props;
---

<svg
  class={cn(
    "text-neutral-500 size-10 md:size-12 -translate-x-3 md:-translate-x-5 mb-4",
    className
  )}
  viewBox="0 0 24 24"
  stroke="currentColor"
  stroke-width="0.6"
  fill="none"
>
  <line x1="12" y1="5" x2="12" y2="19"></line>
  <line x1="5" y1="12" x2="19" y2="12"></line>
</svg>
